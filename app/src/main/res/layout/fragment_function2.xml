<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".ui.function2.function2Fragment">

    <Button
        android:id="@+id/btn_youTube"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="사고대처 영상 시청"
        android:textColor="#000000"
        android:layout_margin="10dp"/>

    <Button
        android:id="@+id/btn_youTube1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="현장 응급처치 영상 시청"
        android:textColor="#000000"
        android:layout_margin="10dp"/>

    <Button
        android:id="@+id/btn_youTube2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="절단 사고 응급처치 영상 시청"
        android:textColor="#000000"
        android:layout_margin="10dp"/>

    <Button
        android:id="@+id/btn_youTube3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="심폐소생술 교육 영상 시청"
        android:textColor="#000000"
        android:layout_margin="10dp"/>

    <Button
        android:id="@+id/btn_youTube4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="출혈 응급처치 영상 시청"
        android:textColor="#000000"
        android:layout_margin="10dp"/>

    <Button
        android:id="@+id/btn_youTube5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="AED 사용법 영상 시청"
        android:textColor="#000000"
        android:layout_margin="10dp"/>

    <Button
        android:id="@+id/btn_youTube6"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="골절시 응급처치 영상 시청"
        android:textColor="#000000"
        android:layout_margin="10dp"/>

    <Button
        android:id="@+id/btn_youTube7"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="화상 응급처치 교육영상 시청"
        android:textColor="#000000"
        android:layout_margin="10dp"/>

</LinearLayout>